<?xml version="1.0" encoding="utf-8"?>

<EnvironmentConfiguration
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema"
  xmlns="http://code.google.com/p/databasescripter/2010/08/29/EnvironmentConfigurationSchema">

  <Name>QA</Name>
  
  <!-- Global script properties. These will be substituted into the scripts if a matching key is found in -->
  <!-- a script. Global properties may be overriden for a script by specifying a property element in a  -->
  <!-- script element below -->
  <Properties>
    <Property name="connection">medialibrary</Property>
    <Property name="databaseName">MediaLibrary_QA</Property>
  </Properties>

  <!-- Script-specific configuration. Scripts can optionally have their own properties element, which allows -->
  <!-- overriding of global script properties. -->
  <ScriptConfigurations>

    <ScriptConfiguration name="insert sample data">
      <Properties>
        <Property name="title">Curb Your Enthusiasm: Meet The Blacks</Property>
        <Property name="episodeNum">s06e01</Property>
        <Property name="date">0x000099A400000000</Property>
        <Property name="description">After Larry''s quest to find the perfect excuse for missing a pair of parties backfires on him big time, he gets in a tense, heated fight with Cheryl. So to make up for it, he agrees to take in the Black Family, who lost their home in Hurricane Edna. But can the Blacks deal with Larry''s total recklessness and lack of respect, not to mention his poorly planned pastry selection?</Property>
        <Property name="rating">4</Property>
        <Property name="mediumType1Name">DVD-R</Property>
        <Property name="mediumType1Description">DVD-R 4.7GB</Property>
        <Property name="mediumType2Name">BD-RE</Property>
        <Property name="mediumType2Description">BD-RE 2.0</Property>
      </Properties>
    </ScriptConfiguration>

  </ScriptConfigurations>

</EnvironmentConfiguration>


